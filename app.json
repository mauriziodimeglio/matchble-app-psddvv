
{
  "expo": {
    "name": "Matchble",
    "slug": "matchble",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/5986e3bc-b857-4410-bbe4-a1b696cf46df.png",
    "scheme": "matchble",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/5986e3bc-b857-4410-bbe4-a1b696cf46df.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F5F5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.matchble.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/5986e3bc-b857-4410-bbe4-a1b696cf46df.png",
        "backgroundColor": "#F5F5F5"
      },
      "package": "com.matchble.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/5986e3bc-b857-4410-bbe4-a1b696cf46df.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#F5F5F5",
          "image": "./assets/images/5986e3bc-b857-4410-bbe4-a1b696cf46df.png",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
